{
  "name": "ng-block-ui",
  "version": "0.9.3",
  "description": "Angular (2 & up) Block UI",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "watchman lib \"npm run build:tsc && cp -r bundles demo\"",
    "build": "npm run build:ngc && npm run build:tsc",
    "build:ngc": "node_modules/.bin/ngc -p tsconfig.json && echo 'Build Complete'",
    "build:tsc": "node_modules/typescript/bin/tsc -p tsconfig-umd.json && echo 'Build Complete'",
    "pack:dev": "npm run build && npm pack",
    "prerelease": "npm run build",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin master && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin master && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin master && git push origin --tags",
    "test": "node_modules/karma/bin/karma start karma.conf.js",
    "test:watch": "node_modules/karma/bin/karma start karma.conf.js --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuuurt13/ng-block-ui.git"
  },
  "keywords": [
    "Angular",
    "Angular 2",
    "Block UI",
    "Spinner"
  ],
  "author": "kuuurt13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuuurt13/ng-block-ui/issues"
  },
  "homepage": "https://github.com/kuuurt13/ng-block-ui#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/compiler-cli": "^2.4.9",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/platform-server": "^2.4.9",
    "@types/jasmine": "^2.5.43",
    "es6-shim": "^0.35.1",
    "html-webpack-plugin": "^2.22.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.1.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.2.1",
    "typings": "^1.3.1",
    "watchman": "^0.1.8",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "zone.js": "~0.6.26"
  },
  "files": [
    "dist",
    "bundles",
    "demo"
  ]
}
